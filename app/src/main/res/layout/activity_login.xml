<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".LoginActivity">
    <com.wcy.client12306.ui.SuperEditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/colorfocus"
        app:ic_left_click="@drawable/ic_left_click"
        app:ic_left_unclick="@drawable/ic_left_unclick"
        app:left_x="0"
        app:left_y="0"
        app:left_width="60"
        app:left_height="60"

        app:ic_delete="@drawable/delete"
        app:delete_x="0"
        app:delete_y="0"
        app:delete_width="80"
        app:delete_height="80"

        app:lineColor_click="@color/colorfocus"
        app:lineColor_unclick="@color/colorDeafult"
        app:linePosition="1"

        app:cursor="@drawable/cursortest"
        />

    <com.wcy.client12306.ui.SuperEditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"

        app:ic_left_click="@drawable/password_click"
        app:ic_left_unclick="@drawable/password_unclick"

        android:textSize="20sp"
        android:inputType="textPassword"

        />

</LinearLayout>